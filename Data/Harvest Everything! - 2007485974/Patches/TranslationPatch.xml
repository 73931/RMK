		<nomatch Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/BodyPartDef[defName="SZ_Spine"]</xpath>
					<nomatch Class="PatchOperationConditional">
						<xpath>/Defs/RecipeDef[defName="InstallNaturalSpine"]</xpath>
						<match Class="PatchOperationSequence">
							<operations>
								<li Class="PatchOperationReplace">
									<xpath>/Defs/RecipeDef[defName="InstallNaturalSpine"]/label</xpath>
									<value>
										<!-- install spine -->
										<label>척추 이식</label>
									</value>
								</li>
								<li Class="PatchOperationReplace">
									<xpath>/Defs/RecipeDef[defName="InstallNaturalSpine"]/description</xpath>
									<value>
										<!-- Install a biological spine. -->
										<description>척추를 이식합니다.</description>
									</value>
								</li>
								<li Class="PatchOperationReplace">
									<xpath>/Defs/RecipeDef[defName="InstallNaturalSpine"]/jobString</xpath>
									<value>
										<!-- Installing spine. -->
										<jobString>척추 이식 중</jobString>
									</value>
								</li>
							</operations>
						</match>
					</nomatch>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/BodyPartDef[defName="SZ_Jaw"]</xpath>
