	<!-- SPINE -->
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/BodyPartDef[defName="SZ_Spine"]</xpath>
		<nomatch Class="PatchOperationConditional">
			<xpath>Defs/ThingDef[defName="Spine"]</xpath>
			<match Class="PatchOperationSequence">
				<operations>
					<li Class="PatchOperationReplace">
						<xpath>Defs/ThingDef[defName="Spine"]/label</xpath>
						<value>
							<!-- Spine -->
							<label>척추</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>Defs/ThingDef[defName="Spine"]/description</xpath>
						<value>
							<!-- A biological human spine. Required for walking and standing. -->
							<description>목과 등, 허리, 엉덩이, 꼬리 부분에 이르기까지 주요 골격을 유지하도록 하는 뼈입니다.</description>
						</value>
					</li>
				</operations>
			</match>
		</nomatch>
	</Operation>

	<!-- JAW -->
	<Operation Class="PatchOperationConditional">
		<xpath>Defs/BodyPartDef[defName="SZ_Jaw"]</xpath>
		<nomatch Class="PatchOperationConditional">
			<xpath>Defs/ThingDef[defName="Jaw"]</xpath>
			<match Class="PatchOperationSequence">
				<operations>
					<li Class="PatchOperationReplace">
						<xpath>Defs/ThingDef[defName="Jaw"]/label</xpath>
						<value>
							<!-- jaw -->
							<label>턱</label>
						</value>
					</li>
					<li Class="PatchOperationReplace">
						<xpath>Defs/ThingDef[defName="Jaw"]/description</xpath>
						<value>
							<!-- A biological human jaw. A nice set of teeth for chewing food and talking. -->
							<description>음식을 씹고 말하기에 좋은 이빨 세트.</description>
						</value>
					</li>
				</operations>
			</match>
		</nomatch>
	</Operation>

	<!-- Compatibility Patch -->
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>The Birds and the Bees</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/ThingDef[defName="ReproductiveOrgans"]</xpath>
					<match Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationReplace">
								<xpath>Defs/ThingDef[defName="ReproductiveOrgans"]/label</xpath>
								<value>
									<!-- reproductive organs -->
									<label>생식 기관</label>
								</value>
							</li>
							<li Class="PatchOperationReplace">
								<xpath>Defs/ThingDef[defName="ReproductiveOrgans"]/description</xpath>
								<value>
									<!-- Biological human reproductive organs. Useful for procreation and recreation , surprisingly interchangeable. -->
									<description>Biological human reproductive organs. Useful for procreation and recreation , surprisingly interchangeable.</description>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<li Class="PatchOperationConditional">
					<xpath>/Defs/RecipeDef[defName="InstallNaturalReproductiveOrgans"]</xpath>
					<match Class="PatchOperationSequence">
						<operations>
							<li Class="PatchOperationReplace">
								<xpath>Defs/RecipeDef[defName="InstallNaturalReproductiveOrgans"]/label</xpath>
								<value>
									<!-- install reproductive organs -->
									<label>생식 기관 이식</label>
								</value>
							</li>
							<li Class="PatchOperationReplace">
								<xpath>Defs/RecipeDef[defName="InstallNaturalReproductiveOrgans"]/description</xpath>
								<value>
									<!-- Install biological reproductive organs. -->
									<description>생식 기관을 이식합니다.</description>
								</value>
							</li>
							<li Class="PatchOperationReplace">
								<xpath>Defs/RecipeDef[defName="InstallNaturalReproductiveOrgans"]/jobString</xpath>
								<value>
									<!-- Installing reproductive organs. -->
									<jobString>생식 기관 이식 중</jobString>
								</value>
							</li>
						</operations>
					</match>
				</li>
				<li Class="PatchOperationFindMod">
					<mods>
						<li>Rah's Bionics and Surgery Expansion</li>
					</mods>
					<match Class="PatchOperationConditional">
						<xpath>/Defs/HediffDef[defName="ReproductiveOrgans"]</xpath>
						<match Class="PatchOperationSequence">
							<operations>
								<li Class="PatchOperationReplace">
									<xpath>Defs/HediffDef[defName="ReproductiveOrgans"]/label</xpath>
									<value>
										<!-- Reproductive Organ transplant -->
										<label>생식 기관 이식체</label>
									</value>
								</li>
								<li Class="PatchOperationReplace">
									<xpath>Defs/HediffDef[defName="ReproductiveOrgans"]/description</xpath>
									<value>
										<!-- A transplanted Reproductive Organ. -->
										<description>이식된 생식 기관입니다.</description>
									</value>
								</li>
							</operations>
						</match>
					</match>
				</li>
