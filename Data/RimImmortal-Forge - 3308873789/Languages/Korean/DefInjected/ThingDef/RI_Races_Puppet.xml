  <RI_Mech_AncientPuppetB.tools.head.label>머리</RI_Mech_AncientPuppetB.tools.head.label>
  <!-- EN: left blade -->
  <RI_Mech_AncientPuppetB.tools.left_blade.label>왼쪽 칼날</RI_Mech_AncientPuppetB.tools.left_blade.label>
  <!-- EN: blade -->
  <RI_Mech_AncientPuppetB.tools.left_blade.labelNoLocation>칼날</RI_Mech_AncientPuppetB.tools.left_blade.labelNoLocation>
