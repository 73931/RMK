<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Biotech</li>
    </mods>
    <match Class="PatchOperationSequence">
      <success>Always</success>
      <operations>
        <li Class="PatchOperationReplace">
          <success>Always</success>
          <xpath>/Defs/RecipeDef[defName="Enforcer"]/label</xpath>
          <value>
            <label>인포서 어썰터 메카노이드 배양</label>
          </value>
        </li>
        <li Class="PatchOperationReplace">
          <success>Always</success>
          <xpath>/Defs/RecipeDef[defName="Enforcer"]/description</xpath>
